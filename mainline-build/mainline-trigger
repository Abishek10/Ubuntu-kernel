#!/bin/bash

set -e

here=`dirname $0`
case "$here" in
/*) ;;
*)  here="`pwd`/$here" ;;
esac

commit="$1"

#
# UBUNTU PRE-PROPOSED: tips of the ubuntu trees.
#
"$here/cod-preproposed-trigger"

#
# MAINLINE TAGS: tagged versions in mainline trees.
#
"$here/cod-tags-trigger"

#
# MAINLINE CRACK: tips of various mainline trees.
#
"$here/cod-crack-trigger"
