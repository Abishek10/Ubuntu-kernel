# test01
# dependency package: none

test01() {
    TEST="BIOS-01: Check for errors in dmidecode information"
    tst_prompt "
\n
You are about to run tests to check for errors in your bios.
\n 
    "
    case $? in 
    0) # yes
        local RC=0
        fwts dmi_decode \
          --results-output=$QALOG/BIOS-test01.log 
        
        grep "Critical failures:" $QALOG/BIOS-test01.log | cut -f4 -d ':' | \
        grep NONE &>/dev/nnull || RC=$(($RC+1))
       
        grep "High failures:" $QALOG/BIOS-test01.log | cut -f4 -d ':' | \
        grep NONE &>/dev/nnull || RC=$(($RC+1))

        tst_reslog $RC "linux" ;;

    *)
        tst_skip ;;
    esac
    return
}
