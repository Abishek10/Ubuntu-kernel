#!/usr/bin/perl

$foo = shift(@ARGV);
open (TIME, "$foo/time") || die;
while(<TIME>) {
	/([\d\.]*)user ([\d\.]*)system (\d*):([\d\.]*)elapsed (\d*)%CPU/;
	$user += $1;
	$system += $2;
	$elapsed += ($3 * 60) + $4;
	$cpu += $5;
	$count++;
}
print "Elapsed: " . $elapsed/$count . "s User: " . $user/$count . "s System: " . $system/$count . "s CPU: " . $cpu/$count . "%\n";
# 189.89user 57.87system 0:22.08elapsed 1121%CPU
