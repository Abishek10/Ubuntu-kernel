cat - >debian.$branch/etc/getabis <<EOM
repo_list=(
	"http://ports.ubuntu.com/pool/main/l/linux-qcm-msm"
)

getall armel st1-5
EOM
cat - >debian.$branch/etc/kernelconfig <<EOM
archs="armel"
family="ubuntu"
EOM

cat - >>debian.$branch/rules.d/armel.mk <<EOM

do_doc_package          = false
do_source_package       = false
do_common_headers_indep = false
do_libc_dev_package     = false
do_tools                = false
EOM

git add debian.$branch/etc debian.$branch/rules.d/armel.mk
